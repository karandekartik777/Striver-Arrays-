arr = [1,2,3,4,11,9,7,6]

def secondmax(arr):
    largest = arr[0]
    slargest = -1
    for i in range(len(arr)):
        if arr[i]>largest:
            slargest = largest 
            largest = arr[i]
        elif arr[i]>slargest and arr[i]<largest :
            slargest = arr[i]
    return slargest 
    
    
arr = [1,2,3,4,11,9,7,6]
    
print(secondmax(arr))

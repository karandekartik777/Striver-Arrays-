def lenoflongersubarray(arr,n,k):
    maxlength = 0
    
    for i in range(0,len(arr)):
        Sum = 0
        for j in range(i,n):
            Sum += arr[j]
            
            if Sum == k:
                maxlength = max(maxlength,j-1+1)
                
    return maxlength
    
    
    
arr = [ 10, 5, 2, 7, 1, 9 ]
n = len(arr)
k = 15
print(lenoflongersubarray(arr,n,k))
